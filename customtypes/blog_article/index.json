{
  "id": "blog_article",
  "label": "Blog Article",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "article_title": {
        "type": "StructuredText",
        "config": {
          "label": "Article Title",
          "placeholder": "",
          "allowTargetBlank": true,
          "single": "heading2"
        }
      },
      "uid": {
        "type": "UID",
        "config": {
          "label": "uid",
          "placeholder": ""
        }
      },
      "article_category": {
        "type": "Select",
        "config": {
          "label": "Article Category",
          "placeholder": "",
          "options": [
            "Guides",
            "Tests",
            "How to"
          ]
        }
      },
      "featured_image": {
        "type": "Image",
        "config": {
          "label": "Featured Image",
          "constraint": {
            "height": 350,
            "width": 1920
          },
          "thumbnails": []
        }
      },
      "article_excerpt": {
        "type": "StructuredText",
        "config": {
          "label": "Article Excerpt",
          "placeholder": "",
          "allowTargetBlank": true,
          "single": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      },
      "article_author_name": {
        "type": "StructuredText",
        "config": {
          "label": "Article Author Name",
          "placeholder": "",
          "allowTargetBlank": true,
          "single": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      },
      "article_author_link": {
        "type": "Link",
        "config": {
          "label": "Article Author Link",
          "placeholder": "",
          "select": null
        }
      },
      "article_author_image": {
        "type": "Image",
        "config": {
          "label": "Article Author Image",
          "constraint": {
            "width": 40,
            "height": 40
          },
          "thumbnails": []
        }
      },
      "article_reading_time": {
        "type": "Number",
        "config": {
          "label": "Article Reading Time",
          "placeholder": ""
        }
      },
      "article_publishing_date": {
        "type": "Date",
        "config": {
          "label": "Article Publishing Date",
          "placeholder": ""
        }
      },
      "article_update_timestamp": {
        "type": "Timestamp",
        "config": {
          "label": "Article Update Timestamp",
          "placeholder": ""
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "featured_articles": {
              "type": "SharedSlice"
            },
            "image": {
              "type": "SharedSlice"
            },
            "text": {
              "type": "SharedSlice"
            },
            "scrolling_cards": {
              "type": "SharedSlice"
            },
            "demo_simple_heading": {
              "type": "SharedSlice"
            },
            "demo_heading_michel": {
              "type": "SharedSlice"
            },
            "dianka_demo_heading": {
              "type": "SharedSlice"
            }
          }
        }
      }
    }
  }
}